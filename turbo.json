{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {},
    "clean": {},
    "check:types": {},
    "test": {
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "build": {
      "outputs": [".next/**", "build/**", "dist**", "node_modules/.cache/metro/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
