{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {},
    "clean": {
      "cache": false
    },
    "check:types": {
      "cache": false
    },
    "test": {
      "inputs": ["**/*.{ts,tsx,js,jsx}"]
    },
    "build": {
      "outputs": [".next/**", "build/**", "dist**", "node_modules/.cache/metro/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    }
  }
}
